{
  "version": 2,
  "builds": [
    {
      "src": "web/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/web/build/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/web/build/index.html"
    }
  ]
} 